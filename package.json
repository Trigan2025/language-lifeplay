{
  "name": "language-lifeplay",
  "version": "0.1.10",
  "license": "",
  "keywords": [
    "lifeplay",
    "lpmod",
    "lp"
  ],
  "description": "Syntax-highlighter for the mod files of a certain nsfw game named LifePlay",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "repository": "https://github.com/Trigan2025/language-lifeplay",
  "main": "./lib/main",
  "configSchema": {
    "enforceCrLf": {
      "description": "Enforce CRLF line-endings",
      "type": "boolean",
      "default": true
    },
    "enforceTabType": {
      "description": "Enforce 4-spaces indentations",
      "type": "boolean",
      "default": true
    }
  }
}
